  "bugs": {
    "url": "https://github.com/sandhawke/webgram/issues"
  },
  "homepage": "https://github.com/sandhawke/webgram#readme",
  "dependencies": {
    "debug": "^3.0.1",
    "eventemitter3": "^2.0.3",
    "express": "^4.15.4",
    "level": "^1.7.0",
    "morgan": "^1.8.2",
    "node-localstorage": "^1.3.0",
    "opn": "^5.1.0",
    "server-destroy": "^1.0.1",
    "teen_process": "^1.10.0",
    "ws": "^3.1.0"
  },
  "devDependencies": {
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
